						format(string, sizeof(string), "Armas: %s.", weaponname);
						SendClientMessageEx(playerid, COLOR_GRAD1, string);
					}
				}
				new cash =  GetPlayerCash(giveplayerid);
				format(string, sizeof(string), "*** Dinero: %d  ***", cash);
