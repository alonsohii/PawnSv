        Info[playerid][pPos_y] = GetPVarFloat(playerid, "rOldY");
        Info[playerid][pPos_z] = GetPVarFloat(playerid, "rOldZ");
    }
	if(Spectate[playerid] < 553)
	{
        Info[playerid][pInt] = GetPVarInt(playerid, "SpecInt");
