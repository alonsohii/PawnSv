{
	new
		Float:oldX,
		Float:oldY,
		Float:oldZ,
		Float:oldRotX,
		Float:oldRotY,
		Float:oldRotZ;

	GetDynamicObjectPos(objectid, oldX, oldY, oldZ);
	GetDynamicObjectRot(objectid, oldRotX, oldRotY, oldRotZ);
 	if(!IsValidDynamicObject(objectid)) return 0;
  	MoveDynamicObject(objectid, x, y, z, 10.0, rx, ry, rz);

	if(response == EDIT_RESPONSE_FINAL)
	{
		CustomTextEnum[CreandoObjetoID][ctX] = x;
        CustomTextEnum[CreandoObjetoID][ctY] = y;
        CustomTextEnum[CreandoObjetoID][ctZ] = z;
        CustomTextEnum[CreandoObjetoID][ctRX] = rx;
        CustomTextEnum[CreandoObjetoID][ctRY] = ry;
        CustomTextEnum[CreandoObjetoID][ctRZ] = rz;

		SetDynamicObjectPos(CustomTextEnum[CreandoObjetoID][ctObjectID], x, y, z);
		SetDynamicObjectRot(CustomTextEnum[CreandoObjetoID][ctObjectID], rx, ry, rz);
		SaveCustomText(CreandoObjetoID);
	}

	if(response == EDIT_RESPONSE_CANCEL)
	{
		SetDynamicObjectPos(objectid, oldX, oldY, oldZ);
		SetDynamicObjectRot(objectid, oldRotX, oldRotY, oldRotZ);

		SendClientMessage(playerid, -1, "Se ha cancelado la edición de objetos.");
	}
	return 1;
}
