		Info[giveplayerid][pJudgeJailTime] = jailtime*60;
		format(string, sizeof(string), "Sentenciaste a %s a cumplir %d minutos en prisión, razón: %s", GetPlayerNameEx(giveplayerid), jailtime, reason);
