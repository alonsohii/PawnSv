		SendClientMessageEx(playerid, COLOR_YELLOW, "Lista de órdenes judiciales pendientes:");

		new
			fCounter,
			string[128];

		foreach(Player, i)
		{
			if(!isnull(Info[i][pWarrant]))
			{
				format(string, sizeof(string), "%s (%d) - razón: %s.",GetPlayerNameEx(i),i,Info[i][pWarrant]);
