	    return 0;
	}
 	if(Info[playerid][pAdminZC] < 2)
	{
		if(strfind(cmdtext, ":", true) != -1)
		{
			new
				i_numcount,
				i_period,
				i_pos;
			while(cmdtext[i_pos])
			{
				if('0' <= cmdtext[i_pos] <= '9') i_numcount++;
