  command(publico, playerid, params[])
	{
		new string[128], phonenumb;
        if(!IsAtCelPublic(playerid)) return SendClientMessageEx(playerid, COLOR_GRAD2, "{FFFF00}[ERROR]:{FFFFFF} No te encuentras en un telefono publico.");
