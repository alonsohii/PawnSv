 	if(GoChase[playerid] < 999){
      GetChased[GoChase[playerid]] = 999;
      GotHit[GoChase[playerid]] = 999;
 	}
	if(GetPVarInt(playerid, "AdvisorDuty") == 1) { Advisors--; }
