			Info[giveplayerid][pMember] = 0;
			Info[giveplayerid][pRank] = 0;
			Info[giveplayerid][pChar] = 0;
			OnDuty[playerid] = 0;
			Info[giveplayerid][pLeader] = 0;
			SetPlayerToTeamColor(giveplayerid);
   			pTazer[giveplayerid] = 0;
		}
	}
	return 1;
}

