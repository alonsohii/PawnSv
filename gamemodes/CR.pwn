					SendClientMessageEx(playerid, COLOR_YELLOW, "* {A2DC35}JOB:{FFFFFF} Ganaste 2 puntos de habilidad por tener horas de doble experiencia.");
				}
				TruckUsed[playerid] = INVALID_VEHICLE_ID;
				DeletePVar(playerid, "TruckDeliver");
			}
		}
	}
