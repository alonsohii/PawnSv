	{

		if(!(1 <= fee <= 10000))
		{
			SendClientMessageEx(playerid, COLOR_WHITE, "Renta minima es $1, maxima es $10,000.");
		}
		else {

			HouseInfo[Info[playerid][pPhousekey2]][hRentFee] = fee;
