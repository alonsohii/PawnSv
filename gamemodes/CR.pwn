	new string[322];
	format(string, 322, "{2F99B5}%s (%d) {FFFFFF}tiene una duda: {FF0000}%s", GetPlayerNameEx(playerid),playerid, Texto);
    for(new i=0;i<MAX_PLAYERS;i++)
