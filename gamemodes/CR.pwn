function BroadCast(color, string[])
{
	foreach(Player, i)
	{
		if(Info[i][pLevel] >= 1 && AdminChat[i] == 0)
		{
		    SendClientMessageEx(i, color, string);
		}
	}
	return 1;
}

function SendVIPMessage(color, string[])
{
	foreach(Player, i)
	{
		if((Info[i][pVIP] >= 1 || Info[i][pAdminZC] >= 2) && Info[i][pVIPCHAT] >= 1)
		{
			SendClientMessageEx(i, color, string);
		}
	}
}

