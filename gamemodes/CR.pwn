                SendClientMessageEx(playerid, COLOR_WHITE, "Este vehículo no tiene ningún registro.");
            }
        }
        else
		{
            SendClientMessageEx(playerid, COLOR_WHITE, "Necesitas estar cerca de dicho vehículo.");
        }
    }
    return 1;
}
