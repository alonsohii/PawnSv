  command(comenzarviaje, playerid, params[])
{
	new vehicleid = GetPlayerVehicleID(playerid);
	new str[128];
    new rand = random(sizeof(RandomSpawn));
	if(!IsAPlanePilot(vehicleid)) return SendClientMessageEx(playerid, COLOR_LIGHTRED,"* Debes estar en el avión indicado para poder comenzar el viaje");
	if(EstaViajando[playerid] == 1) return SendClientMessageEx(playerid, COLOR_LIGHTRED,"* Ya estás en mitad de un viaje, debes terminarlo o abandonarlo (tendrá sus consecuencias)");
	if(Info[playerid][pJob] == 14 || Info[playerid][pJob2] == 14)
	{
		if(TiempoEspera[playerid] > 0)
		{
			format(str,sizeof(str),"Aún no puedes viajar. Debes esperar %d minutos.", TiempoEspera[playerid]);
			SendClientMessageEx(playerid,COLOR_LIGHTRED,str);
			return 1;
		}

		if(LimiteViajes[playerid] >= 5)
		{
		    SendClientMessageEx(playerid,COLOR_YELLOW,"* Has llegado al límite de viajes (5) debes esperar 1 hora para volver a viajar");
		    TiempoEspera[playerid] = 60;
			return 1;
		}
		SetPVarInt(playerid, "Viajando",1);

		AvionElegido[playerid] = GetPlayerVehicleID(playerid);

		SendClientMessageEx(playerid, COLOR_GREEN,"[Aeropuerto Internacional LS] Lleva esta mercancía al punto rojo que te hemos marcado en el radar. Ten cuidado. Suerte.");
		EstaViajando[playerid] = 1;
		LimiteViajes[playerid] ++;
		SetPlayerCheckpoint(playerid, RandomSpawn[rand][0], RandomSpawn[rand][1],RandomSpawn[rand][2], 6.0);
	}
	else SendClientMessageEx(playerid, COLOR_LIGHTRED,"No eres Piloto");
	return 1;
}

 command(abandonarviaje, playerid, params[])
{
	if(EstaViajando[playerid] == 0) return SendClientMessageEx(playerid, COLOR_LIGHTRED, "No hay ningún viaje que abandonar");
	if(Info[playerid][pJob] == 14 || Info[playerid][pJob2] == 14)
	{

        SetVehicleToRespawn(AvionElegido[playerid]);
		EstaViajando[playerid] = 0;
		AvionElegido[playerid] = -1;
		DisablePlayerCheckpoint(playerid);
		SendClientMessageEx(playerid, COLOR_BLUE, "* Has abandonado el viaje, el aeropuerto de LS se molesta contigo por irresponsable. La próxima completa el viaje");
		DeletePVar(playerid, "Viajando");
	}
	else SendClientMessageEx(playerid, COLOR_LIGHTRED,"No eres Piloto");
	return 1;
}

