CMD:neon(playerid,params[])
{
	if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessageEx(playerid, COLOR_RED, "Tienes que estar en un vehículo!");
	new veh = GetPlayerVehicleID(playerid);
	if(GetVehicleOwner(veh) == playerid)
	{
		if(sscanf(params,"s[32]",params[0])) return SendClientMessageEx(playerid, COLOR_YELLOW, "/Neon [Prender / Apagar]");
		switch (YHash(params[0]))
		{
			case _H<prender>:
			{
				if(mostrandoneon[veh] == true) return SendClientMessageEx(playerid, COLOR_RED, "Ya tienes el neon encendido!");
				ShowNeon(playerid, veh);
				SendClientMessageEx(playerid, -1, "Encendiste el neon, cuando quieras apagarlo usa [/neon apagar]");
				mostrandoneon[veh] = true;

			}
			case _H<apagar>:
			{
				DestruirNeon(veh);
				SendClientMessageEx(playerid, -1, "Apagaste el neon, cuando quieras volverlo a encender usa [/neon encender]");
				mostrandoneon[veh] = false;

			}
		}
	}
	else
	{
		SendClientMessageEx(playerid, COLOR_RED, "Este vehículo no es tuyo!");
	}
	return 1;
}

