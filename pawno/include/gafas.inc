Gafas_OnPlayerCommandText(playerid, cmdtext[])
{
	new cmd[128], idx;
	cmd = strtokex(cmdtext, idx);
	
	if(strcmp(cmd,"/gafas",true) == 0)
	{
		if(PlayerInfo[playerid][pGafas] == 0) { SendClientMessage(playerid, Rojo, "* No tienes gafas!"); return 1; }
		format(string,sizeof(string),"* %s se pone unas gafas",pName(playerid));
		if(GetPlayerInterior(playerid) > 0) {
		ProxDetector(12.0, playerid, string,Morado, Morado, Morado, Morado, Morado);}
		else {
		ProxDetector(20.0, playerid, string,Morado, Morado, Morado, Morado, Morado);}
		SendClientMessage(playerid,Blanco,"Usa /qgafas para quitartelas");
		new skin = GetPlayerSkin(playerid);	
		SetPlayerAttachedObject(playerid,GAFAS_SLOT,PlayerInfo[playerid][pGafas], 2, SkinOffSet[skin][0], SkinOffSet[skin][1], SkinOffSet[skin][2], SkinOffSet[skin][3], SkinOffSet[skin][4], SkinOffSet[skin][5], SkinOffSet[skin][6], SkinOffSet[skin][7], SkinOffSet[skin][8]);
		return 1;
	}
	if(strcmp(cmd,"/qgafas",true) == 0)
	{
		if(IsPlayerAttachedObjectSlotUsed(playerid,GAFAS_SLOT))
		{
			format(string,sizeof(string),"* %s se quita las gafas",pName(playerid));
			if(GetPlayerInterior(playerid) > 0) {
			ProxDetector(12.0, playerid, string,Morado, Morado, Morado, Morado, Morado);}
			else {
			ProxDetector(20.0, playerid, string,Morado, Morado, Morado, Morado, Morado);}
			RemovePlayerAttachedObject(playerid,GAFAS_SLOT);
		}
		else
		{
			SendClientMessage(playerid, Rojo, "* No te has puesto las gafas!");
		}
		return 1;
	}
	return 0;
}